 <!DOCTYPE html>
 <html>
  <head>
   <title><%= full_title(yield(:title)) %></title>
   <%= stylesheet_link_tag    "application", :media => "all" %>
   <%= javascript_include_tag "application" %>
   <%= csrf_meta_tags %>
   <%= render 'layouts/shim' %>
  </head>
  <body>
    <%= render 'layouts/header' %>
     <div id="home_wrapper">
      <div class="container">
   	<% flash.each do |key, value| %>
   	 <%= content_tag(:div, value, class: "alert alert-#{key}") %>
   	<% end %>
   	    <div id="home_pic">
   	      <span>
   	        <img src="http://placehold.it/550x315" />
   	      </span>
   	    </div>
   	    <span id="home_action">
 	        <input type="text">
 	        <%= link_to "Pick your Hero", signup_path, class: "btn btn-large btn-primary" %>
 	      </span>
 	      <p id="home_head">Flexitarian mcsweeney's bespoke cliche, raw denim 
 	        biodiesel ethnic<br> fixie. Brunch hella seitan. 
 	      </p>
      </div>
     </div>
     <div class="container"id="home_123">
      <span style="float:left;"><img src="http://placehold.it/200x100" /></span>
      <span style="margin-left:170px;"><img src="http://placehold.it/200x100" /></span>
      <span style="float:right;"><img src="http://placehold.it/200x100" /></span>
     </div>
   <div class="container">
     <%= render 'layouts/footer' %>
     <%= debug(params) if Rails.env.development? %>
   </div>   
  </body>
 </html>
